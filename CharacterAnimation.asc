// Script for all character animations
// Or other global animation scripting

function angry(this Character*)
{
  this.SpeechView = this.GetProperty("angryView");
}

function cool(this Character*)
{
  this.SpeechView = this.GetProperty("normalSpeechView");
}

function awkward(this Character*)
{
  this.SpeechView = this.GetProperty("awkwardView");
}

