function room_Load()
{
  Game.Camera.SetAt(0, 0);
  oEject.SetView(216, 2, 6);
  oReject.SetView(216, 1, 0);
}

function changeRoomWhenDone()
{
  if(oEject.Frame == 0)
  {
    if(cardEjectSound !=null)
    {
      if(!cardEjectSound.IsPaused)
      {
        cardEjectSound.Pause();
      }
    }
  }
}

