function hamishBlockDoor(Object* theDoor, bool blockDoor)
{
  int walkArea = theDoor.GetProperty("walkArea");
  if(blockDoor)
  {
    RemoveWalkableArea(walkArea);    
  } else {
    RestoreWalkableArea(walkArea);
  }
}

